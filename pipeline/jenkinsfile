@Library('JenkinsLib')
import helloworld

pipeline {
    agent any

    stages {
        stage('demo') {
            steps {
                helloworld()
            }
        }
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}